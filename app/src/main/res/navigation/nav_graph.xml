<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.assu_fe_app.HomeFragment"
        android:label="Home">
        <action
            android:id="@+id/action_homeFragment_to_myPartnershipFragment"
            app:destination="@id/myPartnershipDetailsFragment"/>
    </fragment>

    <fragment
        android:id="@+id/locationFragment"
        android:name="com.example.assu_fe_app.presentation.location.LocationFragment"
        android:label="Location">
        <action
            android:id="@+id/action_locationFragment_to_locationSearchFragment"
            app:destination="@id/locationSearchFragment" />
    </fragment>

    <fragment
        android:id="@+id/locationSearchFragment"
        android:name="com.example.assu_fe_app.presentation.location.LocationSearchSuccessFragment"
        android:label="LocationSearchFragment">
        <action
            android:id="@+id/action_locationSearchFragment_to_locationFragment"
            app:destination="@id/locationFragment" />
    </fragment>

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.example.assu_fe_app.DashboardFragment"
        android:label="Dashboard" />

    <fragment
        android:id="@+id/mypageFragment"
        android:name="com.example.assu_fe_app.MypageFragment"
        android:label="Mypage"/>

    <fragment
        android:id="@+id/myPartnershipDetailsFragment"
        android:name="com.example.assu_fe_app.MyPartnershipDetailsFragment"
        android:label="MyPartnershipDetailsFragment">

        <action
            android:id="@+id/action_myPartnershipFragment_to_homeFragment"
            app:destination="@id/homeFragment"/>

    </fragment>


</navigation>