<?xml version="1.0" encoding="utf-8"?>
<layout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_fragment_service_proposal_item_set"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
       >

        <View
            android:id="@+id/view_fragment_service_proposal_mg13"
            android:layout_width="0dp"
            android:layout_height="30dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

        <ImageView
            android:id="@+id/iv_fragment_service_proposal_drop_down_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_service_proposal_box"
            app:layout_constraintTop_toBottomOf="@id/view_fragment_service_proposal_mg13"
            app:layout_constraintStart_toStartOf="parent"/>

        <ImageView
            android:id="@+id/iv_fragment_service_proposal_drop_down_ic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_down"
            app:layout_constraintTop_toTopOf="@id/iv_fragment_service_proposal_drop_down_bg"
            app:layout_constraintEnd_toEndOf="@id/iv_fragment_service_proposal_drop_down_bg"
            app:layout_constraintBottom_toBottomOf="@id/iv_fragment_service_proposal_drop_down_bg"
            android:layout_marginEnd="12.5dp"
            android:backgroundTint="@color/assu_font_sub" />

        <TextView
            android:id="@+id/tv_fragment_service_proposal_drop_down"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="제휴혜택 선택"
            app:layout_constraintTop_toTopOf="@id/iv_fragment_service_proposal_drop_down_bg"
            app:layout_constraintBottom_toBottomOf="@id/iv_fragment_service_proposal_drop_down_bg"
            app:layout_constraintStart_toStartOf="@id/iv_fragment_service_proposal_drop_down_bg"
            android:textSize="13sp"
            android:fontFamily="@font/pretendard_semibold"
            android:textColor="@color/assu_font_sub"
            android:layout_marginStart="10dp"/>

        <View
            android:id="@+id/view_fragment_service_proposal_mg14"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/iv_fragment_service_proposal_drop_down_bg"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.0171"
            />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/ll_fragment_service_proposal_item"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/view_fragment_service_proposal_mg14">

            <ImageView
                android:id="@+id/iv_fragment_service_proposal_num"
                android:layout_width="34dp"
                android:layout_height="31dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/bg_service_proposal_box" />

            <TextView
                android:id="@+id/tv_fragment_service_proposal_num2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="3"
                android:textColor="@color/assu_font_sub"
                android:fontFamily="@font/pretendard_regular"
                android:layout_gravity="center"
                app:layout_constraintTop_toTopOf="@id/iv_fragment_service_proposal_num"
                app:layout_constraintStart_toStartOf="@id/iv_fragment_service_proposal_num"
                app:layout_constraintEnd_toEndOf="@id/iv_fragment_service_proposal_num"
                app:layout_constraintBottom_toBottomOf="@id/iv_fragment_service_proposal_num"/>

            <TextView
                android:id="@+id/tv_fragment_service_proposal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="인 이상일 경우"
                android:textColor="@color/assu_font_sub"
                android:fontFamily="@font/pretendard_regular"
                android:layout_gravity="center"
                app:layout_constraintStart_toEndOf="@id/iv_fragment_service_proposal_num"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="6dp"/>



            <ImageView
                android:id="@+id/iv_fragment_service_proposal_content"
                android:layout_width="100dp"
                android:layout_height="31dp"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/tv_fragment_service_proposal"
                app:layout_constraintBottom_toBottomOf="parent"
                android:background="@drawable/bg_service_proposal_box" />

            <TextView
                android:id="@+id/iv_fragment_service_proposal_content2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="캔콜라"
                android:textColor="@color/assu_font_sub"
                android:fontFamily="@font/pretendard_regular"
                android:layout_gravity="center"
                app:layout_constraintTop_toTopOf="@id/iv_fragment_service_proposal_content"
                app:layout_constraintStart_toStartOf="@id/iv_fragment_service_proposal_content"
                app:layout_constraintEnd_toEndOf="@id/iv_fragment_service_proposal_content"
                app:layout_constraintBottom_toBottomOf="@id/iv_fragment_service_proposal_content"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17sp"
                android:text="제공"
                android:textColor="@color/assu_font_sub"
                android:fontFamily="@font/pretendard_regular"
                android:layout_gravity="center"
                android:layout_marginStart="6dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/iv_fragment_service_proposal_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>