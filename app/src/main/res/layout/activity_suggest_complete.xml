<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/assu_background"
    tools:context=".presentation.user.dashboard.UserSuggestCompleteActivity">


    <View
        android:id="@+id/complete_mg1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0197"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <View
        android:id="@+id/complete_mg2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.0763"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/complete_mg1"/>



    <ImageView
        android:id="@+id/btn_complete_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_suggest_x"
        app:layout_constraintTop_toBottomOf="@id/complete_mg1"
        app:layout_constraintStart_toEndOf="@id/complete_mg2"/>


    <View
        android:id="@+id/complete_mg3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.3438"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_complete_cancel"/>

    <TextView
        android:id="@+id/tv_complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="제출완료됐습니다"
        android:fontFamily="@font/pretendard_regular"
        android:textColor="@color/assu_font_sub"
        android:textSize="20sp"
        android:includeFontPadding="false"
        app:layout_constraintTop_toBottomOf="@id/complete_mg3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/complete_mg4"
        android:layout_width="0dp"
        android:layout_height="20dp"
        app:layout_constraintHeight_percent="0.0202"
        app:layout_constraintStart_toStartOf="@id/tv_complete"
        app:layout_constraintTop_toBottomOf="@id/tv_complete" />

    <TextView
        android:id="@+id/tv_opinion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="소중한 의견"
        android:textColor="@color/assu_font_main"
        android:textSize="30sp"
        android:includeFontPadding="false"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/complete_mg4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/complete_mg5"
        android:layout_width="10dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.01"
        app:layout_constraintStart_toStartOf="@id/tv_complete"
        app:layout_constraintTop_toBottomOf="@id/tv_opinion"/>

    <TextView
        android:id="@+id/tv_thanks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="감사합니다!"
        android:textColor="@color/assu_font_main"
        android:textSize="30sp"
        android:includeFontPadding="false"
        android:fontFamily="@font/pretendard_semibold"
        app:layout_constraintTop_toBottomOf="@id/complete_mg5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>





</androidx.constraintlayout.widget.ConstraintLayout>
</layout>