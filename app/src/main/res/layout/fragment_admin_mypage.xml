<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/assu_background"
    android:layout_height="match_parent"
    tools:context=".presentation.admin.mypage.AdminMypageFragment">

    <View
        android:id="@+id/adm_account_mg1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintWidth_percent="0.0865"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <View
        android:id="@+id/adm_account_mg2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0706"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/adm_account_mg1"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/cv_adm_account_profile"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:cardCornerRadius="30dp"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg2"
        app:layout_constraintStart_toEndOf="@id/adm_account_mg1"
        >

        <ImageView
            android:id="@+id/iv_adm_account_profileImg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop"
            android:src="@drawable/img_location_beer"/>
    </androidx.cardview.widget.CardView>

    <View
        android:id="@+id/adm_account_mg3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintWidth_percent="0.0508"
        app:layout_constraintStart_toEndOf="@id/cv_adm_account_profile"
        app:layout_constraintTop_toTopOf="@id/cv_adm_account_profile"/>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@id/adm_account_mg3"
        app:layout_constraintTop_toTopOf="@id/adm_account_mg3"
        app:layout_constraintBottom_toBottomOf="@id/cv_adm_account_profile">
        <TextView
            android:id="@+id/tv_adm_account_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_semibold"
            android:text="총학생회"
            android:textSize="22sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <View
            android:id="@+id/adm_account_mg4"
            android:layout_width="2dp"
            android:layout_height="0dp"
            app:layout_constraintHeight_percent="0.065"
            app:layout_constraintTop_toBottomOf="@id/tv_adm_account_name"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tv_adm_account_imageEdit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13sp"
            android:textColor="@color/assu_font_sub"
            android:fontFamily="@font/pretendard_medium"
            android:text="사진 수정"
            app:layout_constraintTop_toBottomOf="@id/adm_account_mg4"
            app:layout_constraintStart_toStartOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/adm_account_mg5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0491"
        app:layout_constraintTop_toBottomOf="@id/cv_adm_account_profile"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile"/>

    <TextView
        android:id="@+id/tv_adm_account_setting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="나의 계정 설정"
        android:textColor="@color/assu_font_main"
        android:fontFamily="@font/pretendard_regular"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg5"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile"/>

    <View
        android:id="@+id/adm_account_mg6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0283"
        app:layout_constraintTop_toBottomOf="@id/tv_adm_account_setting"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg6"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile">

        <View
            android:id="@+id/adm_account_mg7_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_adm_alarmSetting"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_account_bell"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg7_1"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/adm_account_mg7_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_adm_alarmSetting"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="알림 설정"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg7_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/adm_account_mg8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0189"
        app:layout_constraintTop_toBottomOf="@id/cl_adm_account_component1"
        app:layout_constraintStart_toStartOf="@id/cl_adm_account_component1"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg8"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile">

        <View
            android:id="@+id/adm_account_mg9_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_adm_logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_account_logout"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg9_1"
            app:layout_constraintTop_toTopOf="parent"/>

        <View
            android:id="@+id/adm_account_mg9_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_adm_logout"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="로그아웃"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg9_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/adm_account_mg12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0441"
        app:layout_constraintTop_toBottomOf="@id/cl_adm_account_component2"
        app:layout_constraintStart_toStartOf="@id/cl_adm_account_component2"/>

    <TextView
        android:id="@+id/tv_adm_account_service_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="고객센터"
        android:fontFamily="@font/pretendard_regular"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg12"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile"/>

    <View
        android:id="@+id/adm_account_mg13"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0283"
        app:layout_constraintTop_toBottomOf="@id/tv_adm_account_service_center"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg13"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile">

        <View
            android:id="@+id/adm_account_mg14_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_folder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_account_folder"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg14_1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <View
            android:id="@+id/adm_account_mg14_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_folder"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="이용약관"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg14_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/adm_account_mg15"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.0189"
        app:layout_constraintTop_toBottomOf="@id/cl_adm_account_component4"
        app:layout_constraintStart_toStartOf="@id/cl_adm_account_component4"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_adm_account_component5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.5012"
        app:layout_constraintTop_toBottomOf="@id/adm_account_mg15"
        app:layout_constraintStart_toStartOf="@id/cv_adm_account_profile">

        <View
            android:id="@+id/adm_account_mg16_1"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ImageView
            android:id="@+id/iv_adm_account_center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_account_counsl"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg16_1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <View
            android:id="@+id/adm_account_mg16_2"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintWidth_percent="0.0761"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/iv_adm_account_center"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:text="고객센터"
            android:fontFamily="@font/pretendard_medium"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/adm_account_mg16_2"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>
</layout>