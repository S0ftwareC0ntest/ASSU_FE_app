<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginBottom="20dp">

    <TextView
        android:id="@+id/tv_item_location_search_rank_count"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1 "
        android:textColor="@color/assu_main"
        android:textSize="14sp"
        android:fontFamily="@font/pretendard_regular"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tv_item_location_search_rank_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="역전할머니 맥주"
        android:textColor="@color/assu_font_main"
        android:textSize="14sp"
        android:fontFamily="@font/pretendard_regular"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/tv_item_location_search_rank_count"
        android:layout_marginStart="8dp" />


</androidx.constraintlayout.widget.ConstraintLayout>